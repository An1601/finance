    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.11",
    "postcss": "^8.4.33",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"

    "@types/node": "^20.12.11",
    "@types/redux-thunk": "^2.1.0",
    "apexcharts": "^3.45.2",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "leaflet": "^1.9.4",
    "moment": "^2.30.1",
    "preline": "^1.9.0",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.51.4",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.21.3",
    "react-toastify": "^10.0.5",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
